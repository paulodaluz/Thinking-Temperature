{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": ["airbnb", "prettier"],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "import/no-unresolved": "off",
    "@typescript-eslint/no-unused-vars": [
      2,
      {
        "args": "none"
      }
    ]
  },
  "env": {
    "commonjs": true,
    "node": true,
    "mocha": true
  }
}
